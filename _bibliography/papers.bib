@article{vrtquartdheure,
  abbr={Article},
  title={Réduction des émissions de la mobilité quotidienne : entre concept de "ville du quart d'heure" et réalités territoriales},
  author={Theureaux, O. and Wester, L. and Chrétien, J. and Louvet, N.},
  abstract={Longtemps oubliée des actions en faveur de la mobilité durable, la réduction des distances parcourues au quotidien est un objectif fort pour réduire les émissions. Le concept de la "ville du quart d'heure" a remis ce sujet au coeur des politiques de mobilité. A travers, l'exemple de la métropole de Aix-Marseille-Provence, nous proposons une analyse territoriale mettant en lumière la complexité de concrétiser cette ambition.},
  journal={VRT},
  location={Paris},
  year={2023},
  month={Décembre}
}
